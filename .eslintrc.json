{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "array-bracket-spacing": "off",
        "indent": [2, 2, {"SwitchCase": 1}],
        "linebreak-style": "off",
        "no-invalid-this": "off",
        "max-len": "warn",
        "no-unused-vars": "warn",
        "object-curly-spacing": "off",
        "operator-linebreak": "warn",
        "prefer-const": "warn",
        "quotes": "warn",
        "valid-jsdoc": "off"
    }
}
